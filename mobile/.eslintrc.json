{
  "env": {
    "react-native/react-native": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/no-unresolved": "off",
    "import/namespace": "off",
    "import/no-duplicates": "off"
  }
}
